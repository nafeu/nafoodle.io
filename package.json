{
  "name": "party-game-starter",
  "version": "0.0.1",
  "description": "Multiplayer party game boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./server",
    "build": "./node_modules/.bin/babel -d ./dist ./server",
    "prebuild": "rm -rf dist/*",
    "start": "NODE_ENV=production node ./dist/index.js",
    "prestart": "npm run build",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "keywords": [
    "socketio",
    "multiplayer",
    "party",
    "game"
  ],
  "author": "Nafeu Nasir <nafeu.nasir@gmail.com> (http://nafeu.com/)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "lodash": "4.17.13",
    "morgan": "1.9.1",
    "number-to-words": "1.2.4",
    "redux": "4.0.1",
    "socket.io": "2.4.0",
    "socket.io-client": "2.2.0",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "nodemon": "1.19.1"
  }
}
